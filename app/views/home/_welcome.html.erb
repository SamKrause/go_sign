<div id="welcome">
  <h1 class="profiletitle">Welcome <%= current_user.first_name %></h1>
  <div class="username">
    <%= image_tag(current_user.avatar.url(:thumb)) %>
  </div>
  <div class="testsLink">
    <%= link_to 'Start Learning ASL!', tests_index_path, :method => :get %>
  </div>
  <div class="testsLink">
    <%= link_to 'Your Stats', users_profile_path, :method => :get %>
  </div>
  <div class="profileLogOut">
    <%= link_to "Log Out", destroy_user_session_path, :id => "logoutbutton", :method => :delete if current_user %><br/>
    <%= link_to 'Edit Profile', edit_user_registration_path, :method => :get %>
  </div>
</div>
